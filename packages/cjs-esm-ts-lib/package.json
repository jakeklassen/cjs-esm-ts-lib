{
  "name": "cjs-esm-ts-lib",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "Jake Klassen",
  "type": "module",
  "license": "MIT",
  "imports": {
    "#app/*": "./src/*"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "tsup src --format cjs,esm --dts",
    "prepare": "ts-patch install -s",
    "watch": "pnpm run build -- --watch src",
    "start": "ts-node src",
    "test": "vitest"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "comment-json": "4.2.2",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "expect": "28.1.3",
    "prettier": "2.7.1",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "ts-patch": "^2.0.1",
    "tsconfig-paths": "4.0.0",
    "tsm": "2.2.1",
    "tsup": "^6.1.3",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "uvu": "0.5.6",
    "vite": "^3.0.0",
    "vitest": "^0.18.0"
  }
}
